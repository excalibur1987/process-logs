{
	"name": "process-logs",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"format:staged": "prettier --write --ignore-path .gitignore $(git diff --cached --name-only --diff-filter=d | grep -E '\\.(ts|svelte)$')",
		"format:push": "prettier --write --ignore-path .gitignore $(git diff --diff-filter=d --name-only origin/master..HEAD | grep -E '\\.(ts|svelte)$')"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.5.27",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"autoprefixer": "^10.4.16",
		"daisyui": "^4.4.19",
		"dotenv": "^16.4.7",
		"postcss": "^8.4.32",
		"prettier-plugin-organize-imports": "^4.1.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.3.6",
		"typescript": "^5.5.0",
		"vite": "^5.4.4"
	},
	"dependencies": {
		"@sveltejs/adapter-node": "^5.2.10",
		"@types/lodash": "^4.14.202",
		"@types/pg": "^8.10.9",
		"date-fns": "^2.30.0",
		"drizzle-kit": "^0.30.0",
		"drizzle-orm": "^0.38.0",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"express": "^4.21.2",
		"lodash": "^4.17.21",
		"postgres": "^3.4.5",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"zod": "^3.23.8"
	},
	"type": "module"
}
